{
    "manifest_version": 3,
    "version": "1.0.0",
    "name": "YouTube Data",
    "description": "Extension to get youtube video details",
  
    "icons": {
      "16": "images/16.png",
      "48": "images/48.png",
      "128": "images/128.png"
    },
  
    "action": {
      "default_icon":  "images/48.png",
      "default_popup":  "index.html"
    },
  
    "permissions": [
      "activeTab",
      "tabs",
      "downloads"
    ]
  
  }
